class Solution {   
    public boolean palindromeCheck(String s) {
        char[] rev=new char [s.length()];  //used int at char string at place of char
        int j=s.length()-1;
        for(int i=0;i<s.length();i++){
            rev[i]=s.charAt(j);  //not used charAt()
            j--;
        }
        String reversed=new String(rev); //[] used 
        return s.equals(reversed);  
    }
}
